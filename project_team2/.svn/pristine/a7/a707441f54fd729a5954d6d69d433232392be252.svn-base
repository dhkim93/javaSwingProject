package com.bs.vo;

public class AdminPageVO {
	private String productCode;
	private String productName;
	private String size;
	private String brane;
	private int prince;
	private String material;
	private String buydate;
	public AdminPageVO() {
		super();
		// TODO Auto-generated constructor stub
	}
	public AdminPageVO(String productCode, String productName, String size,
			String brane, int prince, String material, String buydate) {
		super();
		this.productCode = productCode;
		this.productName = productName;
		this.size = size;
		this.brane = brane;
		this.prince = prince;
		this.material = material;
		this.buydate = buydate;
	}
	public String getProductCode() {
		return productCode;
	}
	public void setProductCode(String productCode) {
		this.productCode = productCode;
	}
	public String getProductName() {
		return productName;
	}
	public void setProductName(String productName) {
		this.productName = productName;
	}
	public String getSize() {
		return size;
	}
	public void setSize(String size) {
		this.size = size;
	}
	public String getBrane() {
		return brane;
	}
	public void setBrane(String brane) {
		this.brane = brane;
	}
	public int getPrince() {
		return prince;
	}
	public void setPrince(int prince) {
		this.prince = prince;
	}
	public String getMaterial() {
		return material;
	}
	public void setMaterial(String material) {
		this.material = material;
	}
	public String getBuydate() {
		return buydate;
	}
	public void setBuydate(String buydate) {
		this.buydate = buydate;
	}
	@Override
	public String toString() {
		return getProductCode()+","+getProductName();
	}
	
	

}
